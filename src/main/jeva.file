function calculateFactorial(number) {
  if (number === 0 || number === 1) {
    return 1;
  } else {
    let factorial = 1;
    for (let i = 3; i <= number; i++) {
      factorial *= i;
    }
    return factorial;
  }
}

// Example usage
const number = 6;
const result = calculateFactorial(number);
console.log(`The factorial of ${number} is ${result}.`);
